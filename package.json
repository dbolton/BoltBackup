{
  "name": "SolidBackup",
  "description": "File Backup for Windows. Free and open source.",
  "version": "0.4.0",
  "license": "GPL-3.0+",
  "author": "David Bolton",
  "main": "main.js",
  "build": {
    "appId": "info.davidbolton.solidbackup",
    "files": [
      "index.html",
      "main.js",
      "main-process/",
      "render-process/",
      "style/",
      "build/wordmark.png"
    ],
    "extraFiles": [
      "Binaries/"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "dbolton",
        "repo": "SolidBackup"
      }
    ],
    "win": {
      "requestedExecutionLevel": "highestAvailable"
    }
  },
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "dev": "./node_modules/.bin/electron . /dev",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "build -p always"
  },
  "devDependencies": {
    "electron": "^1.8.7",
    "electron-builder": "^19.27.3"
  },
  "dependencies": {
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbolton/SolidBackup.git"
  }
}
